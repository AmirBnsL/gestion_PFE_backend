{
  "name": "gestion_pfe_backend",
  "version": "1.0.0",
  "description": "backend for PFE management app",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,css,md}\"",
    "clean": "find ./src -name \"*.js\" -type f -delete",
    "build": "npx tsc",
    "start": "concurrently \"npx tsc --watch\" \"nodemon\"",
    "dev:nodemon": "nodemon --config nodemon.json src/index.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon\"",
    "dev:debug": "concurrently \"tsc --watch\" \"nodemon --exec node --inspect -r ts-node/register --env-file=.env ./src/index.ts\"",
    "swagger": "ts-node --env-file=.env src/swagger.ts",
    "db:create": "ts-node ./node_modules/typeorm-extension/bin/cli.mjs db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/bin/cli.mjs db:drop",
    "seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.mjs -d src/configs/datasource.ts seed:run",
    "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.mjs seed:create"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21",
    "typeorm-extension": "^3.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@faker-js/faker": "^9.6.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-github": "^5.1.8",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  }
}
